{
  "name": "telegram_content",
  "version": "1.0.0",
  "description": "Генератор контента для телеграма",
  "main": "dist/api.js",
  "type": "module",
  "scripts": {
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "vite build",
    "build": "npm run build:server && npm run build:client",
    "start": "node dist/api.js",
    "dev": "ts-node-dev --respawn --transpile-only --project tsconfig.json api.ts",
    "dev:client": "vite",
    "preview:client": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Theoretician9/Telegram_content.git"
  },
  "bugs": {
    "url": "https://github.com/Theoretician9/Telegram_content/issues"
  },
  "homepage": "https://github.com/Theoretician9/Telegram_content#readme",
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.3",
    "@vitejs/plugin-react": "^4.4.1",
    "prisma": "^6.7.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.4"
  },
  "dependencies": {
    "@prisma/client": "^6.7.0",
    "express": "^5.1.0",
    "zod": "^3.24.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "@tanstack/react-query": "^5.2.0",
    "lucide-react": "^0.274.0"
  }
}
